{
  "name": "BringMe",
  "author": "Futurice",
  "description": "React Native App Starter Kit for Android and iOS",
  "license": "MIT",
  "homepage": "http://getpepperoni.com",
  "bugs": "https://github.com/futurice/pepperoni-app-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/futurice/pepperoni-app-kit"
  },
  "keywords": [
    "React Native",
    "Starter Kit",
    "Bitrise",
    "Redux",
    "Auth0"
  ],
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "bundle:ios": "node ./node_modules/react-native/local-cli/cli.js bundle --platform ios --entry-file index.js --bundle-output ios/BringMe/main.jsbundle --dev=false --verbose",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src test",
    "coverage": "rimraf coverage && jest --coverage",
    "version": "support/version-ios.sh",
    "postinstall": "rm ./node_modules/react-native/local-cli/core/__fixtures__/files/package.json"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation)"
    ]
  },
  "rnpm": {
    "assets": [
      "assets/fonts/"
    ]
  },
